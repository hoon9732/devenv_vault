{
  "name": "ICDV",
  "version": "0.1.0",
  "description": "ICD(Interface Control Document) viewer/editor.",
  "author": "David Jean, PGM Systems R&D Team 4, Hanwha Aerospace",
  "private": true,
  "homepage": "./",
  "main": "main/main.js",
  "dependencies": {
    "@blueprintjs/core": "^6.3.1",
    "@blueprintjs/icons": "^6.1.0",
    "@blueprintjs/table": "^6.0.5",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^5.15.15",
    "@mui/material": "^5.18.0",
    "electron-win-state": "^1.1.22",
    "fs-extra": "^11.3.2",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.2.23",
    "react-scripts": "5.0.1",
    "reactflow": "^11.11.4",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron:start": "concurrently -k \"cross-env BROWSER=none NODE_OPTIONS=--no-deprecation npm start\" \"wait-on http://localhost:3000 && cross-env ELECTRON_START_URL=http://localhost:3000 electron .\"",
    "electron:build": "npm run build && electron-builder"
  },
  "build": {
    "appId": "com.example.icdv",
    "productName": "ICDV",
    "files": [
      "build/**/*",
      "main/main.js",
      "preload/preload.js",
      "src/data/**/*"
    ],
    "win": {
      "target": "nsis"
    },
    "extends": null,
    "directories": {
      "buildResources": "assets"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^9.0.0",
    "cross-env": "^7.0.3",
    "electron": "^29.1.5",
    "electron-builder": "^24.13.3",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.6.2",
    "wait-on": "^7.2.0"
  }
}
